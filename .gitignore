# Poetry virtual environment (usually in project root or .venv)
.python-version

# Model files and caches
*.bin
*.safetensors
*.gguf
finetuned_model/
my_finetuned_model/
.cache/

# Tuned models: Exclude weights and checkpoints, but keep metadata for reproducibility
# First, exclude all model weight files in tuned_models/
tuned_models/**/*.safetensors
tuned_models/**/*.bin
tuned_models/**/*.pt
tuned_models/**/*.pth

# Exclude training checkpoints (entire directories)
tuned_models/**/checkpoint-*/

# Exclude training state files
tuned_models/**/optimizer.pt
tuned_models/**/scheduler.pt
tuned_models/**/scaler.pt
tuned_models/**/rng_state.pth
tuned_models/**/training_args.bin
tuned_models/**/trainer_state.json

# Now include metadata files we want to track (order matters - negations must come after exclusions)
# Model registry and README files
!tuned_models/**/model_registry.json
!tuned_models/**/README.md

# LoRA adapter configuration
!tuned_models/**/adapter_config.json

# Model info and metadata
!tuned_models/**/model_info.json

# Tokenizer files (all JSON tokenizer files)
!tuned_models/**/tokenizer.json
!tuned_models/**/tokenizer_config.json
!tuned_models/**/vocab.json
!tuned_models/**/merges.txt
!tuned_models/**/added_tokens.json
!tuned_models/**/special_tokens_map.json

# Chat template
!tuned_models/**/chat_template.jinja

# Other config files
!tuned_models/**/*.toml
!tuned_models/**/Modelfile
